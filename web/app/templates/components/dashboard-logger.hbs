<div class="col-md-6">
  <div class='box box-primary'>
    <div class="box-header with-border">
      <h3 class="box-title">Logger</h3>
    </div>

    <form>
      <div class="box-body box-logger">
        {{#each logs key='timestamp' as |l|}}
          <p>{{l.timestamp}} - {{l.message}}</p>
        {{/each}}
      </div>
    </form>
  </div>
</div>